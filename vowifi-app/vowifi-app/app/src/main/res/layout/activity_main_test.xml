<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivityTest">
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    <Button
        android:id="@+id/tmpid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:enabled="true"
        android:text="start vpn"
        tools:ignore="HardcodedText"/>


    <!--<Button-->
        <!--android:id="@+id/tmpidgui"-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:text="gui" />-->

    <TextView
        android:id="@+id/tmpid2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="state"
        tools:ignore="HardcodedText"/>

    <Button
        android:id="@+id/tmpidIncCall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="incoming call!"
        tools:ignore="HardcodedText"/>

    <Button
        android:id="@+id/tmpidSipReg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="register"
        tools:ignore="HardcodedText"/>

    <Button
        android:id="@+id/tmpidSipUnrReg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Unregister"
        tools:ignore="HardcodedText"/>

    <Button
        android:id="@+id/tmpSipCall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="call"
        tools:ignore="HardcodedText"/>


    <TextView
        android:id="@+id/vifiVolteState"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/volteState"/>

    <Button
        android:id="@+id/tmpDialogShow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="show pager"
        tools:ignore="HardcodedText"/>

        <Button
            android:id="@+id/buttonRssiToasts"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="rssi Toast"
            tools:ignore="HardcodedText"/>

    <Button
        android:text="provisioning"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:id="@+id/btn_provisioning"
        tools:ignore="HardcodedText"/>

    <Button
        android:text="new password"
        android:layout_width="181dp"
        android:layout_height="wrap_content"
        android:id="@+id/btn_new_password"
        tools:ignore="HardcodedText"/>

    <Button
        android:text="new_certificate"
        android:layout_width="181dp"
        android:layout_height="wrap_content"
        android:id="@+id/btn_new_certificate"
        tools:ignore="HardcodedText"/>
    <TextView
        android:id="@+id/tv_request"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="request"
        tools:text="request"
        tools:ignore="HardcodedText"/>
    <TextView
        android:id="@+id/tv_results"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="result"
        tools:text="result"
        tools:ignore="HardcodedText"/>
        <Button
            android:text="Button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/button3"
            tools:ignore="HardcodedText"/>
    </LinearLayout>
</ScrollView>
</LinearLayout>
